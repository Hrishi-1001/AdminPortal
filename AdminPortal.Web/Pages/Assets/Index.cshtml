@page
@model AdminPortal.Web.Pages.Assets.IndexModel
@{
}

<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Assets[0].ID)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Assets[0].State)
			</th>
		</tr>
	</thead>
	<tbody>
@foreach(var asset in Model.Assets)
{
	<tr>
		<td>@asset.ID</td>
		<td>@asset.State</td>
	</tr>
}
	</tbody>
</table>

